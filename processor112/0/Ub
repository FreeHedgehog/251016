FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      Ub;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (6 0 0);
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    top
    {
        type            symmetryPlane;
    }
    side1
    {
        type            symmetryPlane;
    }
    side2
    {
        type            symmetryPlane;
    }
    procBoundary112to48
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary112to96
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary112to113
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


