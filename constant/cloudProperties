FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          cloudProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationSchemes
{
    U               cellPointFace;
}

g 		    (0 0 -9.81);

dragModel   SyamlalOBrien;

subCycles	1;

diffusionBandWidth 2e-4;

