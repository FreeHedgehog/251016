FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          transportProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

continuousPhaseName  air;

rho.air           1.225;

transportModel      Newtonian;

nu                  1.5e-5;

rhoa            rhoa [1 -3 0 0 0 0 0] 2650; //Density of the particles

rhob            rhob [1 -3 0 0 0 0 0] 1.225; //Density of the air

nub             nub [0 2 -1 0 0 0 0] 1.5e-5; // viscousity of the air

da              da [0 1 0 0 0 0 0] 0.00017; // diameter of the particle

Cvm             Cvm [0 0 0 0 0 0 0] 0.0; // 无量纲系数，用于速度方程（UEqn）中的某个附加项/源项加权。

Cl              Cl [0 0 0 0 0 0 0] 0; // 无量纲升力系数（通常用于颗粒在流场中的升力模型，如 Saffman lift）。

Ct              Ct [0 0 0 0 0 0 0] 1; // 无量纲湍流相关系数，当前求解流程中“已定义但未使用”，设置为 1 对结果没有影响，仅占位。

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //