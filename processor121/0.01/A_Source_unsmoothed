FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.01";
    object      A_Source_unsmoothed;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform (-0 0 0);

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    top
    {
        type            symmetryPlane;
    }
    side1
    {
        type            symmetryPlane;
    }
    side2
    {
        type            symmetryPlane;
    }
    procBoundary121to57
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary121to105
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary121to120
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary121to122
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


